<uwsgi>
  <socket>127.0.0.1:3031</socket>
  <pythonpath><PROJECTPATH>/releases/current/<APP_NAME></pythonpath>
  <home><PROJECTPATH></home>
  <wsgi-file><PROJECTPATH>/releases/current/<APP_NAME>/wsgi.py</wsgi-file>
  <processes>8</processes>
  <uid>www-data</uid>
  <gid>www-data</gid>
  <pidfile><PROJECTPATH>/pid/uwsgi.pid</pidfile>
  <buffer-size> 32768</buffer-size>
  <master/>
  <harakiri>360</harakiri>  
  <env>DJANGO_SETTINGS_MODULE=settings.production</env>
  <env>SECRET_KEY=<DJANGOSECRETKEY></env>
  <env>DBNAME=<DBNAME></env>
  <env>DBUSER=<DBUSER></env>
  <env>DBPASS=<DBPASS></env>
  <env>DBHOST=<DBHOST></env>
  <env>DBPORT=<DBPORT></env>
  <env>DOMAIN_NAME=<DOMAIN_NAME></env>
  <env>AWS_ACCESS_KEY_ID=<AWS_ACCESS_KEY_ID></env> 
  <env>AWS_SECRET_ACCESS_KEY=<AWS_SECRET_ACCESS_KEY></env> 
  <env>AWS_STORAGE_BUCKET_NAME=<AWS_STORAGE_BUCKET_NAME></env> 
</uwsgi>
